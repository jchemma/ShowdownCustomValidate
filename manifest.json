{
	"manifest_version": 2,
	"name": "OBA Validator",
	"version": "0.5",
	"icons": {
		"128": "logo.png"
	},
	"background" : {
		"scripts" : ["background.js"]
	},
	"content_scripts" : [{
		"js": ["jquery-3.2.1.min.js", "banned.js", "validationLib.js"],
		"matches": ["http://*/*", "https://*/*"]
	}],
	"browser_action": {
		"default_icon": "logo.png"
	},
	"permissions": ["activeTab"]

}
